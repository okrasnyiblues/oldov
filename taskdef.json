{
    "family": "hello-world",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "hello-world",
            "essential": true,
            "memory": 512,
            "cpu": 500,
            "portMappings": [
                {
                    "containerPort": 9090,
                    "hostPort": 80
                }
            ]
        }
    ]
}
